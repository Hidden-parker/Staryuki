print(f"{y}Ejemplo-> google.com, facebook.com")
pa = input("link aqui -> ")
f = open("loginadmin.txt", "r")
print(f"{v}Cargando...")
for linea in f:
    url = (f"https://{pa}{linea}")
    try:
        conn = urllib.request.urlopen(url)
    except urllib.error.HTTPError as e:

        pass
    except urllib.error.URLError as e:
        print("URL mal escrita o no existe")
        input("Presiona para salir")
        exit()
    else:
        print(f"https://{pa}{linea}")
    

f.close()

